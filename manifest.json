{
	"manifest_version": 2,

	"name": "Taylor Swift Institute",
  "short_name": "Taylor Swift",
	"description": "Taylor Swift everything",
	"author": "Sam Pikesley",
	"version": "1.3",
	"homepage_url": "https://github.com/pikesley/taylor-swift-institute",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
